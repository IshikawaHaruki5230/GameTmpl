.root {
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 10px;
}

.extends-button {
    background-color: #4CAF50;
    color: white;
    font-size: 16px;
    padding: 10px 20px;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    text-align: center;
}

.extends-button:hover {
    background-color: #45a049;
}